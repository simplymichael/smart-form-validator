{
  "check-coverage": true,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [ 
    "**/*.test.js",
    "node_modules/"
  ],
  "extension": [
    ".js"
  ],
  "reporter": [ 
    "clover",
    "html",
    "json", 
    "lcov",
    "text-summary"
  ],
  "branches": 75,
  "lines": 75,
  "functions": 75,
  "statements": 75
}